{
    "name": "mario-jump",
    "description": "A Phaser 3 template using Vite.",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite --config vite/config.dev.mjs",
        "build": "vite build --config vite/config.prod.mjs",
